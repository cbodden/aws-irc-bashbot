_filters()
{
    > ${TMP_FILE1}

    _FILTER=()
    _FILTER+=(${CMD})
    _FILTER+=(${ARG})

    local _CNT=0
    while [[ "${_CNT}" -lt "${#_FILTER[@]}" ]]
    do
        if [[ "${_FILTER[${_CNT}]}" == "word" ]]
        then
            printf "%s\n" \
                "working" \
                | tee -a ${TMP_FILE1} > /dev/null
        fi
        let _CNT=_CNT+1
    done
    source core/chan_text_out.shlib
    _chan_text_out
}
