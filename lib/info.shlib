# help text - this shows git info
function .info()
{
    printf "%s\n" \
        "${PROGNAME%.*} is an irc bot written in bash." \
        "source can be found at : https://github.com/cbodden/irc-bashbot" \
        "currently at commit $(git rev-parse HEAD)" \
        | tee ${TMP_FILE1} > /dev/null
    _chan_text_out
}
